{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env":{
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "react/prop-types": 0,
    "no-warning-comments": 1,
    "no-unused-vars": 1,
    "linebreak-style": 0,
    "no-nested-ternary": 0,
    "class-methods-use-this": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
